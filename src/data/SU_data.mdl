include ..\model\lists

#####################
###### USE SIDE #####
#####################

Q|O|[c] := MGR|O|[c] + CI|O|[c] + CH|O|[c] + G|O|[c] + I|O|[c] + X|O|[c] + DS|O|[c]

CI|O|[c] := sum(CI|O|[c, s] on s)

I|O|[c] := sum(IA|O|[c, s] on s)

|V|[c] := |V|D[c] + |V|M[c] where V in Q CH G I DS X

|V||O| := sum(|V||O|[c] on c) where V in Q CH G I X DS CI MGR

|V| := |V|D + |V|M where V in Q CH G I X DS CI MGR

#####################
###### SUPPY SIDE ###
#####################

MGP|O|[c] := sum(MGP|O|[cc, c] on cc)

YQ[c]*PYQ[c] + PTAXCD[c]*TAXCD[c] + PMGPD[c]*MGPD[c] := PQD[c]*QD[c]

YQbis[c] + TAXCD[c] + MGPD[c] := QD[c]

M[c]*PM[c] + PTAXCM[c]*TAXCM[c] + PMGPM[c]*MGPM[c] := PQM[c]*QM[c]

Mbis[c] + TAXCM[c] + MGPM[c] := QM[c]

MGPD := sum(MGPD[c] on c) 

MGPM := sum(MGPM[c] on c) 

MGR[cc] := sum(MGP[cc, c] on c)

MGP[cc, c] := MGPD[cc, c] + MGPM[cc, c]

M := sum(M[c] on c)

CI|O|[s] := sum(CI|O|[c, s] on c)

CI[s] := CID[s] + CIM[s]











### GDP definitions

GDP := CH + G + I + X + DS - M

GDP[c] := CH[c] + G[c] + I[c] + X[c] + DS[c] - M[c]

GDPbis := sum(GDP[c] on c)

