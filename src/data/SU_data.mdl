include ..\model\lists

Q|O|[c] := MGR|O|[c] + CI|O|[c] + CH|O|[c] + G|O|[c] + I|O|[c] + X|O|[c] + DS|O|[c]

CI|O|[c] := sum(CI|O|[c, s] on s)

I|O|[c] := sum(IA|O|[c, s] on s)

|V|[c] := |V|D[c] + |V|M[c] where V in Q CH G I DS

X[c] := XD[c] + XM[c]

|V||O| := sum(|V||O|[c] on c) where V in Q CH G I X DS CI MGR

|V| := |V|D + |V|M where V in Q CH G I X DS CI MGR











M := sum(M[c] on c)

### GDP definitions

GDP := CH + G + I + X + DS - M

GDP[c] := CH[c] + G[c] + I[c] + X[c] + DS[c] - M[c]

GDPbis := sum(GDP[c] on c)

