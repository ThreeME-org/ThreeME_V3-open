include ..\model\lists

########################################################
### Hypothesis for the long term of model
########################################################

### Population growth rate
GR_POP    := 0.0029

### Growth rate of labor technical progress 
GR_PROG_L := 0.0170

### Growth rate of capital technical progress
GR_PROG_K := 0.0

### Growth rate of energy technical progress
GR_PROG_E := 0.0


### Inflation (growth rate of prices)
GR_PRICES := 0.0200

### Risk premium
RISKPREM := 0.0100

### Growth rate of the real economy
GR_REAL := (1 + GR_POP)*(1 + GR_PROG_L) - 1

### Growth rate of the nominal economy
GR_NOMI := (1 + GR_POP)*(1 + GR_PROG_L)*(1 + GR_PRICES) - 1

### Growth rate of the nominal wage
GR_NOMIW := (1 + GR_PRICES)*(1 + GR_PROG_L) - 1

### Interest rate
R := GR_NOMI - RISKPREM

### Grothw rate of technical progress in sectors
GR_PROG_L[s] := GR_PROG_L
GR_PROG_E[s] := GR_PROG_E
GR_PROG_K[s] := GR_PROG_K

##############
### Indexes
##############
P 		:=  1 * (1 + GR_PRICES) ^ (@year - %baseyear)
PROG_L 	:=  1 * (1 + GR_PROG_L) ^ (@year - %baseyear)

PROG_L[s] := 1 * (1 + GR_PROG_L[s]) ^ (@year - %baseyear)
PROG_E[s] := 1 * (1 + GR_PROG_E[s]) ^ (@year - %baseyear)
PROG_K[s] := 1 * (1 + GR_PROG_K[s]) ^ (@year - %baseyear)


##############
### Supply side
##############

### Production of commodity c by sector s at basic price        
Y[c, s] := SUT($c,$s) * (1 + GR_REAL) ^ (@year - %baseyear)

### Margins paid to commodity cc on the domestic commodity c 
MGPD[cc, c] := SUT($c+17,$cc+7) * (1 + GR_REAL) ^ (@year - %baseyear)

### Margins paid to commodity cc on the imported commodity c 
MGPM[cc, c] := SUT($c+26,$cc+7) * (1 + GR_REAL) ^ (@year - %baseyear)

### Net taxes on commodity c domestically produced
NTAXPD[c] := SUT($c+17,13) * (1 + GR_REAL) ^ (@year - %baseyear)
# NTAXPD_VAL[c] := SUT($c+16,13) * (1 + GR_NOMI) ^ (@year - %baseyear)

### Net taxes on imported commodity c
NTAXPM[c] := SUT($c+26,13) * (1 + GR_REAL) ^ (@year - %baseyear)
# NTAXPM_VAL[c] := SUT($c+25,13) * (1 + GR_NOMI) ^ (@year - %baseyear)

### Net taxes on the production of sector s
NTAXI[s] := SUT(8,$s) * (1 + GR_REAL) ^ (@year - %baseyear)
NTAXI_VAL[s] := SUT(8,$s) * (1 + GR_NOMI) ^ (@year - %baseyear)

### Super bruto wages paid by sector s (including employees and employers' social contribution)

### Bruto wages paid by sector s including employees (but not employers)' social contribution
WAGES[s] := SUT(9,$s) * (1 + GR_REAL) ^ (@year - %baseyear)            

### Employers' social security contribution paid by sector s
### RSSC = employeRs' Social Security Contribution 
RSSC[s] := SUT(10,$s) * (1 + GR_REAL) ^ (@year - %baseyear) 

### Labor of sector s 
L[s] := SUT(11,$s) * (1 + GR_POP) ^ (@year - %baseyear)

### Depreciation rate of the capital of sector s
Tdec[s] := SUT(13,$s) 
# TO DO. Use an english name: TDepr

### Capital stock of sector s 
K[s] := I[s] * (1 + GR_REAL) / (Tdec[s] + GR_REAL)

############################
### Use side (domestic)
############################

### Domesticly produced margins recieved by commodity c 
MGRD[c] := SUT($c+17,14) * (1 + GR_REAL) ^ (@year - %baseyear)

### Intermediary raw material consumption domestically produced c of sector s
CID[c, s] := SUT($c+17,$s+14) * (1 + GR_REAL) ^ (@year - %baseyear)

CHD[c] := SUT($c+17,21) * (1 + GR_REAL) ^ (@year - %baseyear)
GD[c]  := SUT($c+17,22) * (1 + GR_REAL) ^ (@year - %baseyear)

ID[c, s] := SUT($c+17,$s+22) * (1 + GR_REAL) ^ (@year - %baseyear)

XD[c]  := SUT($c+17,29) * (1 + GR_REAL) ^ (@year - %baseyear)
DSD[c] := SUT($c+17,30) * (1 + GR_REAL) ^ (@year - %baseyear)

############################
### Use side (imported)
############################

### Imported margins recieved by commodity c
MGRM[c] := SUT($c+26,14) * (1 + GR_REAL) ^ (@year - %baseyear)

### Impoted intermediary raw material consumption c of sector s
CIM[c, s] := SUT($c+26,$s+14) * (1 + GR_REAL) ^ (@year - %baseyear)        

CHM[c] := SUT($c+26,21) * (1 + GR_REAL) ^ (@year - %baseyear)
GM[c]  := SUT($c+26,22) * (1 + GR_REAL) ^ (@year - %baseyear)

IM[c, s] := SUT($c+26,$s+22) * (1 + GR_REAL) ^ (@year - %baseyear)

DSM[c] := SUT($c+26,30) * (1 + GR_REAL) ^ (@year - %baseyear)
XM[c]  := SUT($c+26,29) * (1 + GR_REAL) ^ (@year - %baseyear)


############################
### DEMOGRAPHY DATA
############################

POP := WAPop/0.6
WAPop := LF/0.7
LF := L/(1 - 0.1)
empl := L


########################################
### Calibration of parameters
########################################

################################
### Elastiscity of substitutions
################################

## Between energy intermediary consumption
ES_NRJ[ce, cee, s] := 0.3

## Between domestic and imported commodities for margins recieved, private and public consumption, investment and exports
ES_|V|M[c] := 0.6 where V in MGR CH G I X

ES_|V|M[c, s] := 0.6 where V in CI I

################################
### Other elastiscities
################################

# In the wage equation
RHO_W_P[s] := 1
RHO_W_PROG[s] := 0
RHO_W_U[s] := 0.2
RHO_W_DU[s] := 0.65
RHO_W_L[s] := 0

RHO_W_Cons[s] := log(1+GR_NOMIW) - RHO_W_P[s]*log(1+GR_PRICES) - RHO_W_PROG[s]*log(1+GR_PROG_L) + RHO_W_U[s] * UnR 


# In the labor force participation rate equation
RHO_PART := 0.3