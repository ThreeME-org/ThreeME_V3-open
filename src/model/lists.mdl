# List of sectors
%list_sec := AIND ATPT ATPO ASER ANRE ANFO #[s]

# List of commodities
%list_com := CIND CTRP CSER CNJN CNJM #[c]

# List of material commodities
%list_com_MAT := CIND CTRP CSER   #[cm]

# List of transport commodities
%list_com_TRSP := CTRP   #[ct]

# List of energy commodities
%list_com_E := CNJN CNJM  #[ce]

# List of other material commodities
%list_com_MATO := CIND CSER   #[cmo]

# List of production factors
%list_F := K L E MAT  #[f]

# List of Green House Gases (GHG)
%list_GHG := CO2 CH4 N2O SF6 HFC PFC #[ghg]



# Default iterators
s in %list_sec

c in %list_com
cc in %list_com
ccc in %list_com

cm in %list_com_MAT

ct in %list_com_TRSP
ctt in %list_com_TRSP

cmo in %list_com_MATO

ce in %list_com_E
cee in %list_com_E


f in %list_F
ff in %list_F

ghg in %list_GHG

O in D M

%baseyear := 2000
%firstdate := 1998