#-----------------------------------------------------AUTOMOBILE-----------------------------------------------------------------

# Equation H.4.78

AUTO_elec[h,DES,pcl] = (@year>%baseyear)*(delta_AUTO[h,CA,DES] * AUTO_elec[h,pcl]{-1}) + (@year<=%baseyear)*AUTO_elec[h,DES,pcl]{-1}*(1+STEADYSTATE(2,1))        

AUTO_th[h,DES,ecl] = (@year>%baseyear)*(delta_AUTO[h,ecl,DES] * AUTO_th[h,ecl]{-1})+ (@year<=%baseyear)*AUTO_th[h,DES,ecl]{-1}*(1+STEADYSTATE(2,1))          

AUTO[h,DES,ecl] = (@year>%baseyear)*(AUTO_th[h,DES,ecl]) + (@year<=%baseyear)*AUTO[h,DES,ecl]{-1}*(1+STEADYSTATE(2,1)) 

@over AUTO[h,DES,CA] = (@year>%baseyear)*(sum(AUTO_elec[h,DES,pcl] on pcl) + AUTO_th[h,DES,CA]) + (@year<=%baseyear)*AUTO[h,DES,CA]{-1}*(1+STEADYSTATE(2,1))           

AUTO[h,DES] = (@year>%baseyear)*(sum(AUTO[h,DES,ecl] on ecl)) + (@year<=%baseyear)*AUTO[h,DES]{-1}*(1+STEADYSTATE(2,1))                    #modifié

# Equation H.4.79 and H.4.80

NewAUTO[h,CA] = NewAUTO_elec[h] + NewAUTO_th[h,CA]   #modifié

NewAUTO[h,c] = NewAUTO_th[h,c]  where c in %list_ener_class \ CA

# Verif

AUTO_verif[h] = sum( AUTO[h,ecl] on ecl )

AUTO_verif = sum( AUTO_verif[h] on h )

Verif_AUTO = AUTO_verif - AUTO

Verif_AUTO2 = ( @year > %baseyear ) * (NewAUTO - AUTO_DES - d(AUTO)) + ( @year <= %baseyear ) * Verif_AUTO2{-1}


# Equation H.4.81

EXP_MOBAUTO_Val[h,ecl] = ( @year > %baseyear ) * (DEBT_AUTO_Val[h,ecl]{-1} * ( R_I_AUTO[h,ecl]{-1} + R_RMBS_AUTO[h,ecl]{-1} ) +   _
                                R_CASH_AUTO[h,ecl] * PNewAUTO[h,ecl] * NewAUTO[h,ecl] * ( 1 - R_SUB_AUTO[h,ecl] ) +       _
                                PEXP_AUTO[h,ecl] * EXP_AUTO[h,ecl] ) +                                                    _
                         ( @year <= %baseyear ) * (EXP_MOBAUTO_Val[h,ecl]{-1} * ( 1 + STEADYSTATE(1,1) ) )

R_LOAN_AUTO[h,ecl] = 1 - R_CASH_AUTO[h,ecl]

GR_PENER_auto_e[h,ecl] = ADJUST(73,2)*GR_PENER_auto_e[h,ecl]{-1} + (1 - ADJUST(73,2)) * @pchy(PENER_auto[h,ecl])

#phi_NewAUTO[h,CA]*NewAUTO = share_elec*NewAUTO + phi_th[h,CA]*NewAUTO_th[h]        

#phi_NewAUTO[h,c] = phi_th[h,c]   where c in %list_ener_class \CA

PENER_AUTO[h,ecl] * KM_AUTO[h,ecl] = PEXP_AUTO[h,ecl] * EXP_AUTO[h,ecl] if AUTO[h,ecl] <> 0
  
# Equation H.4.83

DEBT_auto_Val[h,ecl] = (@year > %baseyear)*( DEBT_auto_Val[h,ecl]{-1}*(1 - R_RMBS_AUTO[h,ecl]) _
                                             + R_LOAN_AUTO[h,ecl]*PNewAuto[h,ecl]*NewAuto[h,ecl]*(1 - R_SUB_AUTO[h,ecl]) ) _
                       + (@year <= %baseyear)*(DEBT_auto_Val[h,ecl]{-1}*(1 + STEADYSTATE(1,1)))

# Equation H.4.84

EXP_NEWAUTO_VAL[h,ecl] = PNewAuto[h,ecl] * NewAUTO[h,ecl]

# Equation H.4.88

dlog(AUTO[h])= (@year > %baseyear)*(d(log(Km_auto[h]))-d(log(kmPerAuto[h]))) + _
              (@year <= %baseyear)*log(1+STEADYSTATE(2,1))                                     #modifié

# Equation H.4.89 and H.4.90

KM_AUTO[h,ecl] = sum(km_AUTO[h, ecl, ce] on ce)    #modifié

dlog(EXP_AUTO[h,ecl,ce2]) = d(log(AUTO_th[h,ecl,ce2])) -dlog(PROG_AUTO)  if AUTO_th[h,ecl,ce2] <> 0   #modifié

EXP_AUTO[h,CA,23] = sum(EXP_AUTO_elec[h,pcl] on pcl)                                             #modifié

EXP_AUTO[h,c,23] = 0   where c in %list_ener_class \CA                                           #modifié


# -------------------------------------------Aggregation of automobile expenditure--------------------------------------------
# Equation H.4.91

EXP_AUTO[h,ecl] = sum( EXP_AUTO[h,ecl,ce] if EXP_AUTO[h,ecl,ce] <> 0 on ce )

PEXP_AUTO[h,ecl]*EXP_AUTO[h,ecl] = sum( PEXP[ce,h]*EXP_AUTO[h,ecl,ce] if EXP_AUTO[h,ecl,ce] <> 0 on ce )      # corrigé

# Equation H.4.92

@pv EXP_AUTO[h] = sum( EXP_AUTO[h,ecl] if EXP_AUTO[h,ecl] <> 0 on ecl )

EXP_AUTO[ce] = sum( EXP_AUTO[ecl,ce] if EXP_AUTO[ecl,ce] <> 0 on ecl )

# Equation H.4.93

EXP_AUTO[h,ce] = sum( EXP_AUTO[h,ecl,ce] if EXP_AUTO[h,ecl,ce] <> 0 on ecl )

# Equation H.4.95

@pv EXP_AUTO = sum( EXP_AUTO[h] on h )

EXP_AUTO[ecl, ce] = sum(EXP_AUTO[h, ecl, ce] on h)


# -----------------------------------------------------------AGGREGATION AUTOMOBILE---------------------------------------------------
# Equation H.4.96
AUTO[bcl] = sum(AUTO[h, bcl] on h)

# Equation H.4.97
AUTO = sum(AUTO[ecl] on ecl)

# Equation H.4.98

NewAUTO[h] = (@year>%baseyear)*(d(AUTO[h]) + AUTO[h,DES]) + (@year<=%baseyear)*(NewAUTO[h]{-1}*(1+STEADYSTATE(2,1)))  #  modifié

NewAUTO = sum(NewAUTO[h] on h)

 # Equation H.4.99
 @over PnewAUTO[h]*NewAUTO[h] = PAUTO_elec[h]*NewAUTO_elec[h] + PNewAUTO_th[h]*NewAUTO_th[h]  if NewAUTO[h]>0         #modifié
 
# Equation H.4.100
EXP_NEWAUTO_VAL[h] = sum(EXP_NewAUTO_VAL[h, ecl] on ecl)

# Equation H.4.101
EXP_NEWAUTO_VAL = sum(EXP_NEWAUTO_VAL[h] if EXP_NEWAUTO_VAL[h] <> 0 on h)

# Equation H.4.102

phi_exp_03bis[h] = EXP_NEWAUTO_VAL[h] / EXP_NEWAUTO_VAL

# Equation H.4.103

EXP_MOBAUTO_Val[h] = sum(EXP_MOBAUTO_Val[h, ecl] if EXP_MOBAUTO_Val[h, ecl] <> 0 on ecl)

# Equation H.4.104

EXP_MOBAUTO_Val = sum(EXP_MOBAUTO_Val[h] if EXP_MOBAUTO_Val[h] <> 0 on h)

# Equation H.4.105

EXP_03_OTH_VAL = sum(EXP_03_OTH_VAL[h] if EXP_03_OTH_VAL[h] <> 0 on h)

# Equation H.4.107 and H.4.108

d(log(EXP_03_OTH_VAL[h])) = d(log(DISPINC_VAL[h])) + d(log(1 - MPS_HH[h]))

# ------------------------------------------------------TRANSPORT COLLECTIF--------------------------------------------------------------
# Equation H.4.113

EXP_MOB_OTH_Val[h] = sum(PEXP[trsp, h] * EXP[trsp, h] on trsp in %list_trsp_travel)

# ---------------------------------------------------------Total mobility---------------------------------------------------
# Equation H.4.117

EXP_MOB_Val[h] = EXP_MOBAuto_Val[h] + EXP_MOB_OTH_Val[h] + EXP_03_OTH_VAL[h]


 # Modal shift in transportation
 
@over d(log(MAT[16, s])) = d(log(MAT[s])) + d(SUBST_MAT[16, s]) + log(1 -  modal_shift_freight * (1 + 1/4) * MAT[16, s]{-1} / MAT[16, s]{-1}) if MAT[16, s] <> 0

@over d(log(MAT[14, s])) = d(log(MAT[s])) + d(SUBST_MAT[14, s]) + log(1 +  modal_shift_freight             * MAT[16, s]{-1} / MAT[14, s]{-1}) if MAT[14, s] <> 0

@over d(log(MAT[17, s])) = d(log(MAT[s])) + d(SUBST_MAT[17, s]) + log(1 + (modal_shift_freight / 4)        * MAT[16, s]{-1} / MAT[17, s]{-1}) if MAT[17, s] <> 0
 
# Aspect sobriété dans l'automobile et efficacité énérgetique     modifié
 # Equation H.4.89 and H.4.90

AUTO[h,ecl,ce] = AUTO[h,ecl]*SHARE_AUTO[h,ecl,ce]      

AUTO[ce] = sum(AUTO[h,ce] on h)

SHARE_AUTO[h,CA,23] = AUTO_elec[h]/AUTO[h,CA]

SHARE_AUTO[h, c, 23] = 0      where c in %list_ener_class \ CA

SHARE_AUTO[h,CA,ce2] = (1-SHARE_AUTO[h,CA,23])*@elem(AUTO[h,CA,ce2]/(AUTO[h,CA,21] +AUTO[h,CA,22]+AUTO[h,CA,24]),%baseyear)  

# Subventions automobile ( Bonus-malus auto + bonus elec)

SUB_AUTO_VAL[h, ecl] = R_SUB_AUTO[h, ecl]*PNewAUTO[h, ecl] * NewAUTO[h, ecl]

SUB_AUTO_VAL[h] = sum(SUB_AUTO_VAL[h,ecl] on ecl)

SUB_AUTO_VAL = sum(SUB_AUTO_VAL[h] on h)


###########Niveau 1 : Détermination du nombre de km pour l'avion, les trajets longue distance et les trajets courte distance       modifié

 ## passager-kilomètre Avion 
 
 @over d(log(Km_traveler[18, h])) =(@year>%baseyear)*(1.5*d(log(dispinc_val/PCH))-0.7*d(log(PCH_18)-log(PCH))) + _
                                   (@year<=%baseyear)*log(1+STEADYSTATE(2,1))
      
 ## passager-kilomètre Longue distance
 
 d(log(km_traveler_LD_h01)) =(@year>%baseyear)*(0.6*d(log(dispinc_val/PCH)) -0.4*((km_traveler_14[h]{-1}/( km_traveler_14[h]{-1}+ km_trav_auto_LD[h]{-1})) _
